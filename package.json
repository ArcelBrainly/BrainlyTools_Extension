    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "colors": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "event-stream": "^4.0.1",
