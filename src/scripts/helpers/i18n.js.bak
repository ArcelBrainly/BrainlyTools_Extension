import ext from "../utils/ext";

export default keyword => {
	let extension_id;
	if (typeof document.extension != "undefined") {
		extension_id = document.extension.id
	} else {
		extension_id = ext.runtime.id
	}
	console.log("i18n requesting");
	let asd = await Promise.all([ ext.runtime.sendMessage(extension_id, { action: "i18n", data: keyword }, function (res) {
		console.log(res);
		return res
	})])
	return asd;
};